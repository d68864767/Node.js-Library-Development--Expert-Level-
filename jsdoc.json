{
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.ts(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "default",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    }
}
